{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "nycProjectFactory"
		},
		"ls_db_2021_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_db_2021'"
		},
		"ls_dlsyn_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_dlsyn'"
		},
		"ls_ds_data_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_ds_data'"
		},
		"ls_syn_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_syn'"
		},
		"ls_dlsyn_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlnycsyn.dfs.core.windows.net/"
		},
		"ls_ds_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://nycproj.dfs.core.windows.net/"
		},
		"ls_syn_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://synstorge.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "nycProjectFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_db_2021_connectionString": {
						"value": "[parameters('ls_db_2021_connectionString')]"
					},
					"ls_dlsyn_accountKey": {
						"value": "[parameters('ls_dlsyn_accountKey')]"
					},
					"ls_ds_data_accountKey": {
						"value": "[parameters('ls_ds_data_accountKey')]"
					},
					"ls_syn_accountKey": {
						"value": "[parameters('ls_syn_accountKey')]"
					},
					"ls_dlsyn_properties_typeProperties_url": {
						"value": "[parameters('ls_dlsyn_properties_typeProperties_url')]"
					},
					"ls_ds_data_properties_typeProperties_url": {
						"value": "[parameters('ls_ds_data_properties_typeProperties_url')]"
					},
					"ls_syn_properties_typeProperties_url": {
						"value": "[parameters('ls_syn_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "nycProjectFactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'nycProjectFactory_ArmTemplate_0')]"
			]
		}
	]
}